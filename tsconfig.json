{
	"compilerOptions": {
		"target": "esnext",
		"module": "es2015",
		"jsx": "react",
		"moduleResolution": "node",
		"noEmit": true,
		"strict": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"types": ["node", "react"],
		"baseUrl": "src"
	},
	"include": ["src", "gatsby-*"]
}
