{
  "name": "blog.sapegin.me",
  "private": true,
  "license": "See license in <Readme.md>",
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "pretest": "npm run lint:js && npm run lint:css && npm run lint:md",
    "test": "npm run typecheck",
    "posttest": "npm run format",
    "typecheck": "tsc",
    "lint:css": "stylelint 'src/**/*.tsx'",
    "lint:md": "remark --quiet \"content/**/*.md\"",
    "lint:js": "eslint . --cache --fix --ext .js,.tsx",
    "format": "prettier --loglevel warn --write \"**/*.{js,md}\"",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "sync": "node scripts/sync.js",
    "deploy": "npm run sync && npm run build"
  },
  "engines": {
    "node": ">=8.9"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "just-group-by": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-group": "^3.0.2",
    "react-helmet": "^6.1.0",
    "romanize": "^1.1.1",
    "styled-components": "^5.3.0",
    "tamia": "^5.10.1",
    "tamia-gatsby-link": "^1.0.6"
  },
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.2",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-tamia": "^7.2.6",
    "eslint-plugin-react": "^7.24.0",
    "fs-extra": "^10.0.0",
    "gatsby": "^3.9.1",
    "gatsby-plugin-canonical-urls": "^3.9.0",
    "gatsby-plugin-feed": "^3.9.0",
    "gatsby-plugin-goatcounter": "^0.4.0",
    "gatsby-plugin-netlify": "^3.9.0",
    "gatsby-plugin-react-helmet": "^4.9.0",
    "gatsby-plugin-sitemap": "^4.5.0",
    "gatsby-plugin-styled-components": "^4.9.0",
    "gatsby-plugin-ts-config": "^1.1.5",
    "gatsby-plugin-typescript": "^3.9.0",
    "gatsby-remark-prismjs": "^5.6.0",
    "gatsby-remark-twitter-cards": "^0.6.1",
    "gatsby-source-filesystem": "^3.9.0",
    "gatsby-transformer-remark": "^4.6.0",
    "glob": "^7.1.7",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "lodash": "^4.17.21",
    "prettier": "^2.3.2",
    "react-styleguidist": "^11.1.7",
    "remark-cli": "^8.0.0",
    "remark-lint": "^7.0.0",
    "richtypo": "^4.1.1",
    "richtypo-rules-en": "^4.0.7",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.3.5",
    "unist-util-visit": "^2.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,tsx,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,tsx}": [
      "eslint --fix",
      "stylelint",
      "git add"
    ]
  }
}
